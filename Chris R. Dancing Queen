    def _gopigo3_command_forward10in(self):
        #part uno 
        beat = 60/101

         #intro 
        self.gopigo3.set_speed(600)  
        self.gopigo3.spin_left   
        self.gopigo3.steer(15, 90) 
        sleep (3*beat)   
        self.gopigo3.stop()   
        sleep (0.5)          
        self.gopigo3.steer(20, 100)   
        sleep (5*beat)
        self.gopigo3.stop()
        sleep (1) 
        self.gopigo3.set_speed(900)   
        self.gopigo3.steer(0,100)
        sleep (4*beat) 
        self.gopigo3.stop() 
        sleep (1)  
        self.gopigo3.steer(30,100) 
        sleep (6) 
        #self.gopigo3.steer(100,0)
        self.gopigo3.spin_left() 
        sleep (5*beat)  
        self.gopigo3.steer(20,90)  
        sleep (4*beat)
        self.gopigo3.set_speed(900)  
        self.gopigo3.steer(50,20) 
        sleep (3*beat)  
        self.gopigo3.stop() 
        sleep (1)   
        self.gopigo3.backward() 
        sleep(1*beat)  
        self.gopigo3.steer(50,20) 
        self.gopigo3.spin_left()
        sleep(2*beat)
        self.gopigo3.spin_right()
        sleep(4*beat)
        self.gopigo3.stop()
        sleep(4*beat)  
        self.gopigo3.steer(-40,-90) 
        sleep(4*beat)
        self.gopigo3.steer(-10,-80)
        sleep(3*beat)
        self.gopigo3.backward() 
        sleep(6*beat)
        self.gopigo3.stop()
        sleep(1*beat)
        self.gopigo3.spin_left()
        sleep(1*beat)
        self.gopigo3.spin_right()
        sleep(1*beat) 
        self.gopigo3.stop()
        sleep(3*beat)
        self.gopigo3.spin_left()
        sleep(1*beat)
        self.gopigo3.spin_right()
        sleep(1*beat) 
        self.gopigo3.forward()
        sleep(4*beat)
        self.gopigo3.backward()
        sleep(2*beat)
        self.gopigo3.steer(50,100) 
        sleep(4*beat) 
        self.gopigo3.stop()
        sleep(4*beat)
        self.gopigo3.spin_right()
        sleep(2*beat) 
        self.gopigo3.stop()
        sleep(3*beat) 
        self.gopigo3.spin_left() 
        sleep(2*beat) 
        self.gopigo3.backward()
        sleep(3*beat)
        self.gopigo3.steer(20,90) 
        sleep(6*beat) 
        self.gopigo3.stop() 
        sleep(3*beat)
        self.gopigo3.steer(0,50)
        sleep(2*beat)
        self.gopigo3.steer(50,0) 
        sleep(2*beat) 
        self.gopigo3.steer(0,-100) #anyone can be that guyy 
        sleep(2*beat) 
        self.gopigo3.backward()
        sleep(4*beat)
        self.gopigo3.stop() 
        sleep(1*beat) 
        self.gopigo3.steer(0,-60)  #musics hiiigh
        sleep(3*beat)
        self.gopigo3.backward()
        sleep(4*beat) 
        self.gopigo3.spin_right()
        sleep(2*beat)
        self.gopigo3.stop()
        sleep(3*beat) 
        self.gopigo3.backward()
        sleep(0.5*beat)
        self.gopigo3.stop()
        sleep(1*beat) 
        self.gopigo3.backward()
        sleep(0.5*beat)
        self.gopigo3.stop()
        sleep(1*beat) 
        self.gopigo3.backward()
        sleep(0.5*beat)
        self.gopigo3.stop()
        sleep(1*beat) 
        self.gopigo3.backward()
        sleep(0.5*beat)
        self.gopigo3.stop()
        sleep(1*beat) 
        self.gopigo3.backward()
        sleep(1*beat)
        self.gopigo3.stop()
        sleep(1*beat)
        self.gopigo3.spin_right()
        sleep(6*beat) 
         #CHORUSS ENDING
        self.gopigo3.forward()
        sleep(6*beat) 
        self.gopigo3.steer(100,0) 
        sleep(8*beat)  

        self.gopigo3.stop() 

        
